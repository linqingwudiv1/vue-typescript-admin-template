$lq-primary = #409EFF;
$lq-success = #67C23A;
$lq-danger = #F56C6C;
$lq-warning =#E6A23C;
$lq-info = #909399;

// 主边框色
$lq-border-1 = #DCDFE6;
// 二级边色
$lq-border-2 = #E4E7ED;
// 三级边色
$lq-border-3 = #EBEEF5;
// 四级边色·
$lq-border-4 = #F2F6FC;

//背景色
$lq-bg-1 = #f0f0f0;

$lq-padding-1 = 12pt;
$lq-padding-2 = 8pt;
// 背影框
.lq-box-shadow
  box-shadow: 0 2pt 4pt rgba(0, 0, 0, 0.12), 0 0 6pt rgba(0, 0, 0, 0.04)
  border-radius:5pt;

  // 滚动条.
::-webkit-scrollbar-track-piece
    box-shadow   : inset 0 0 3pt rgba(0, 0, 0, 0.1);
    border-radius: 5pt;
    background   : #ededed;
::-webkit-scrollbar
  width : 10pt;  
  height: 10pt;
::-webkit-scrollbar-thumb
  border-radius: 5pt;
  box-shadow   : inset 0 0 3pt rgba(0, 0, 0, 0.1);
  background   : #838383;
::-webkit-scrollbar-thumb:hover
    -webkit-border-radius:10pt;
    background   : #636363;